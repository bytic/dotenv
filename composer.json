{
  "name": "bytic/dotenv",
  "description": "bytic dotenv wrapper",
  "license": "MIT",
  "authors": [
    {
      "name": "Gabriel Solomon",
      "email": "hello@gabrielsolomon.ro"
    }
  ],
  "require": {
    "bytic/utility": "^1.0",
    "symfony/dotenv": "^5.0 || ^6.0"
  },
  "require-dev": {
    "bytic/phpqatools": "^1.0",
    "symfony/console": "^4.0 || ^5.0 || ^6.0"
  },
  "minimum-stability": "dev",
  "prefer-stable": true,
  "autoload": {
    "psr-4": {
      "ByTIC\\Dotenv\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "ByTIC\\Dotenv\\Tests\\Fixtures\\": "tests/fixtures",
      "ByTIC\\Dotenv\\Tests\\": "tests/src"
    }
  },
  "config": {
    "allow-plugins": {
      "phpstan/extension-installer": true,
      "ergebnis/composer-normalize": true,
      "dealerdirect/phpcodesniffer-composer-installer": true,
      "bytic/phpqatools": true
    }
  }
}
